package test4;

import java.io.*;

public class test4 {


		   public static void main(String args[]){
		      int x = 10;

		      do{
		         System.out.print("value of x : " + x );
		         x++;
		         System.out.print("\n");
		         //another single line
		      }while( x < 20 );
		   }
		}
		
